{
    "name": "Future FIMP integration",
    "version": "0.1.5",
    "slug": "futurehome_fimp",
    "description": "Futurehome FIMP integration",
    "url": "https://github.com/runelangseid/hassio-futurehome2mqtt",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "before",
    "boot": "auto",
    "homeassistant_api": true,
    "options": {
        "fimpserver": "futurehome.local",
        "fimpusername": "mqttuser",
        "fimppassword": "mqttpassword",
        "fimpport": 1884,
        "client_id": "fimptoha"
      },
      "schema": {
        "fimpserver": "str",
        "fimpusername": "str",
        "fimppassword": "str",
        "fimpport": "int",
        "client_id": "str"
      }
  }
