{
    "name": "Future FIMP integration",
    "version": "0.2.2",
    "slug": "futurehome_fimp",
    "description": "Futurehome FIMP integration",
    "url": "https://github.com/runelangseid/hassio-futurehome2mqtt",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "boot": "auto",
    "init": false,
    "homeassistant_api": true,
    "options": {
        "fimpserver": "cube.home",
        "fimpusername": "mqttuser",
        "fimppassword": "mqttpassword",
        "fimpport": 1884,
        "client_id": "fimp2mqtt",
        "debug": false,
        "selected_devices": ""
      },
      "schema": {
        "fimpserver": "str",
        "fimpusername": "str",
        "fimppassword": "str",
        "fimpport": "int",
        "client_id": "str",
        "debug": "bool",
        "selected_devices": "str"
      }
}
